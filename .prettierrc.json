{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "always",
  "quoteProps": "consistent",
  "semi": false,
  "singleQuote": false,
  "svelteAllowShorthand": true,
  "svelteIndentScriptAndStyle": true,
  "tabWidth": 2,
